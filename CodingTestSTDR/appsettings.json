{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsCacheOptions": {
    "MaxSize": 8192,
    "AbsoluteExpirationRelativeToNow": "00:03:00"
